<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data noupdate="1">
        <record id="sequence_l10n_ve_control_number" model="ir.sequence">
            <field name="name">Numero de control</field>
            <field name="code">account.move.document.number</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">00-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_tax_retencion_iva" model="ir.sequence">
            <field name="name">Retención de IVA en compras</field>
            <field name="code">account.tax.withholding</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_tax_retencion_islr" model="ir.sequence">
            <field name="name">Retención ISLR</field>
            <field name="code">account.tax.withholding</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>